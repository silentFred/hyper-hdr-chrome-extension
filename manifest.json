{
  "manifest_version": 3,
  "name": "HyperHDR Control",
  "version": "1.0",
  "description": "Control your HyperHDR lights",
  "permissions": ["storage"],
  "host_permissions": ["http://localhost:8090/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}